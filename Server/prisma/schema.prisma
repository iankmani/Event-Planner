generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String @id @default(uuid())
  firstname   String @map("First_Name")
  lastname    String @map("Last_Name")
  email       String @unique @map("Email_Address")
  phonenumber Int    @map("Phone_Number")
  password    String

  @@map("Users_Table")
}
